pipeline {
    agent { label 'my-defined-label' }
    triggers {
        upstream(upstreamProjects: 'starter project', threshold: hudson.model.Result.SUCCESS)
        cron('*/45 * * * 1-5')
        pollSCM('20 */4 * * 0,6')
    }
    stages {
        stage('git') {
            steps{
               git 'https://github.com/nagaraju0/java11-examples.git' 
            }
        }
        satge('build') {
            steps{
                sh '/usr/local/apache-maven-3.8.4/bin/mvn clean package'
            }
        }
    }
}










