node('jdk11-mvn3.8.4') {
    properties([parameters([choice(choices: ['kscripted', 'kdeclarative', 'master'], description: 'this is for build run', name: 'BRANCH_TO_BUILD')])])
    stage('git') {
        git url : 'https://github.com/nagaraju0/java11-examples.git'
    }
    stage('build') {
        sh '/usr/local/apache-maven-3.8.4/bin/mvn clean package'
    }
    stage('archive') {
        archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
    }
    stage('publish test results') {
        junit '**/TEST-*.xml'
    }
}